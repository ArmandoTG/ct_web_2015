@model IEnumerable<Entidades.EFCF.Product>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateProduct")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MakeFlag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FinishedGoodsFlag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Color)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SafetyStockLevel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReorderPoint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StandardCost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ListPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Size)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Weight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DaysToManufacture)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductLine)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Class)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Style)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellStartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellEndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiscontinuedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductModel.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductSubcategory.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SizeUnitMeasure.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WeightUnitMeasure.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    @Html.Partial("_ProductRow", item)
 }

</table>
